<Application x:Class="Rawr.WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:UI="clr-namespace:Rawr.UI;assembly=Rawr.UI"
    StartupUri="WindowMain.xaml">
    <Application.Resources>
      <ResourceDictionary>
        <UI:IconPathConverter x:Key="IconPathConverter"/>
        <UI:GemColorConverter x:Key="GemColorConverter"/>
        <UI:ItemQualityConverter x:Key="ItemQualityConverter"/>
        <UI:PercentConverter x:Key="PercentConverter"/>
        <UI:GemVisibilityConverter x:Key="GemVisibilityConverter"/>
        <UI:GemNameConverter x:Key="GemNameConverter"/>
        <UI:EnchantNameConverter x:Key="EnchantNameConverter"/>
        <UI:EnchantVisbilityConverter x:Key="EnchantVisbilityConverter"/>
      </ResourceDictionary>
    </Application.Resources>
</Application>
