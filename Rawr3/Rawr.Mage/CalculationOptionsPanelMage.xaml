                        <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="5" IsChecked="{Binding Mode=TwoWay, Path=UseFireWard, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height"/>
                        <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,0" VerticalAlignment="Top" Width="Auto" Text="Use Frost Ward: *" TextWrapping="Wrap" ToolTipService.ToolTip="Allows use of Frost Ward every 30 seconds." Grid.Row="6"/>
                        <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="6" IsChecked="{Binding Mode=TwoWay, Path=UseFrostWard, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height"/>
                    </Grid>
                </Grid>
            </controls:TabItem>
            <controls:TabItem Header="Advanced">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.6*" />
                        <ColumnDefinition Width="0.2*" />
                        <ColumnDefinition Width="0.2*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Grid.Row="0" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Global Optimizations: *" TextWrapping="Wrap" ToolTipService.ToolTip="Disables uncommon cooldown and spell combination based on current talent spec to increase performance. In case of uncommon fight conditions this might have to be disabled to achieve accurate results." />
                    <TextBlock Grid.Column="0" Grid.Row="1" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Incremental Optimizations: *" TextWrapping="Wrap" ToolTipService.ToolTip="Heavily restricts cooldown stacking and spell combinations when computing solutions of incremental changes." />
                    <TextBlock Grid.Column="0" Grid.Row="2" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Force Incremental Optimizations: *" TextWrapping="Wrap" ToolTipService.ToolTip="Forces incremental optimizations even when they would have to be disabled for overall accuracy." />
                    <TextBlock Grid.Column="0" Grid.Row="3" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Text="Reconstruct Sequence: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable to calculate the timing of different abilities that best match the solution. Only enable when needed as it is computationally expensive." d:LayoutOverrides="GridBox" />
                    <TextBlock Grid.Column="0" Grid.Row="4" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Enable Hasted Evocation: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enables use of evocation on last tick of Icy Veins or Heroism, should not be used without advanced constraints." />
                    <TextBlock Grid.Column="0" Grid.Row="5" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Max Use Assumption: *" TextWrapping="Wrap" ToolTipService.ToolTip="Add stacking constraints based under assumption that each effect must be used maximum possible amount. You should only disable this when using maximum settings for advanced constraints." />
                    <TextBlock Grid.Column="0" Grid.Row="6" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Cycle Level Haste Procs: *" TextWrapping="Wrap" ToolTipService.ToolTip="Evaluates cycle level haste proc interactions, but at a significant performance cost." />
                    <TextBlock Grid.Column="0" Grid.Row="7" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Patch 3.3.3: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enables 3.3.3 calculations." />
                    <TextBlock Grid.Column="0" Grid.Row="8" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="SMP Computation Limit: *" TextWrapping="Wrap" ToolTipService.ToolTip="Limits the number of iterations taken by SMP algorithm." />
                    <TextBlock Grid.Column="0" Grid.Row="9" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="SMP Search Method: *" TextWrapping="Wrap" ToolTipService.ToolTip="If Best Bound method does not provide solution at the specified computation limit then using Depth First search will provide at least a feasible solution, although not optimal." />
                    <TextBlock Grid.Column="0" Grid.Row="10" Height="Auto" HorizontalAlignment="Right" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Comparison / Display: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable options for comparisons or for display only." Grid.ColumnSpan="3" />
                    <TextBlock Grid.Column="0" Grid.Row="11" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Integral Mana Consumables: *" TextWrapping="Wrap" ToolTipService.ToolTip="Do not allow partial uses of mana consumables." />
                    <TextBlock Grid.Column="0" Grid.Row="12" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Segment Cooldowns: *" TextWrapping="Wrap" ToolTipService.ToolTip="Segment cooldowns into 30 second intervals." />
                    <TextBlock Grid.Column="0" Grid.Row="13" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Advanced Constraints: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enables advanced constraints that are too expensive for comparisons, but are needed for sequence reconstruction." />
                    <TextBlock Grid.Column="0" Grid.Row="14" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Segment Duration: *" TextWrapping="Wrap" ToolTipService.ToolTip="Duration of segments when variable segment duration is not used." />
                    <TextBlock Grid.Column="0" Grid.Row="15" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Variable Segment Duration: *" TextWrapping="Wrap" ToolTipService.ToolTip="Determine segment durations based on cooldown durations." />
                    <TextBlock Grid.Column="0" Grid.Row="16" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Additional Segment Splits: *" TextWrapping="Wrap" ToolTipService.ToolTip="A comma-separated list of segment splits in seconds, useful in combination with debug segmentation." />
                    <TextBlock Grid.Column="0" Grid.Row="17" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Lower Bound Hint: *" TextWrapping="Wrap" ToolTipService.ToolTip="A lower bound supplied to guide depth-first and hybrid search, if you use this make sure it is an actual lower bound." />
                    <CheckBox Grid.Column="2" Grid.Row="1" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=IncrementalOptimizations, UpdateSourceTrigger=Default}" />
                    <CheckBox Grid.Column="2" Grid.Row="2" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=ForceIncrementalOptimizations, UpdateSourceTrigger=Default}" />
                    <CheckBox Grid.Column="2" Grid.Row="3" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=ReconstructSequence, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height, VerticalMargin" />
                    <CheckBox Grid.Column="2" Grid.Row="4" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=EnableHastedEvocation, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height, VerticalMargin" />
                    <CheckBox Grid.Column="2" Grid.Row="5" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=MaxUseAssumption, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height, VerticalMargin" />
                    <CheckBox Grid.Column="2" Grid.Row="6" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=AdvancedHasteProcs, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height, VerticalMargin" />
                    <CheckBox Grid.Column="2" Grid.Row="7" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=Mode333, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height, VerticalMargin" />
                    <CheckBox Grid.Column="1" Grid.Row="11" Margin="0,2,8,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=ComparisonIntegralMana, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height" HorizontalAlignment="Right" />
                    <ComboBox Grid.Column="1" Grid.Row="9" Margin="0,2,0,2" Height="Auto" Grid.ColumnSpan="2" SelectedItem="{Binding Mode=TwoWay, Path=MIPMethod}">
                        <local:MIPMethod>BestBound</local:MIPMethod>
                        <local:MIPMethod>DepthFirst</local:MIPMethod>
                        <local:MIPMethod>HybridGuided</local:MIPMethod>
                        <local:MIPMethod>HybridUnguided</local:MIPMethod>
                    </ComboBox>
                    <TextBox Grid.Column="1" Grid.Row="16" Height="20" Margin="0,2,0,2" Text="{Binding Mode=TwoWay, Path=AdditionalSegmentSplits, UpdateSourceTrigger=Default}" TextWrapping="Wrap" Grid.ColumnSpan="2" />
                    <TextBox Grid.Column="1" Grid.Row="17" Height="20" Margin="0,2,0,2" Text="{Binding Mode=TwoWay, Path=LowerBoundHint, UpdateSourceTrigger=Default}" TextWrapping="Wrap" Grid.ColumnSpan="2" />
                    <CheckBox Grid.Column="2" Grid.Row="0" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=SmartOptimization, UpdateSourceTrigger=Default}" />
                    <TextBox Grid.Column="1" Grid.Row="8" Height="20" Margin="0,2,0,2" Text="{Binding Mode=TwoWay, Path=MaxHeapLimit, UpdateSourceTrigger=Default}" TextWrapping="Wrap" Grid.ColumnSpan="2" />
                    <CheckBox Grid.Column="2" Grid.Row="11" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=DisplayIntegralMana, UpdateSourceTrigger=Default}" />
                    <CheckBox Grid.Column="1" Grid.Row="12" Margin="0,2,8,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=ComparisonSegmentCooldowns, UpdateSourceTrigger=Default}" HorizontalAlignment="Right" />
                    <CheckBox Grid.Column="2" Grid.Row="12" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=DisplaySegmentCooldowns, UpdateSourceTrigger=Default}" />
                    <CheckBox Grid.Column="2" Grid.Row="15" Margin="0,2,0,2" Content="" IsChecked="{Binding Mode=TwoWay, Path=VariableSegmentDuration, UpdateSourceTrigger=Default}" />
                    <ComboBox Grid.Column="1" Grid.Row="14" Margin="0,2,0,2" Height="Auto" Grid.ColumnSpan="2" SelectedItem="{Binding Mode=TwoWay, Path=FixedSegmentDuration}">
                        <System:Int32>10</System:Int32>
                        <System:Int32>15</System:Int32>
                        <System:Int32>20</System:Int32>
                        <System:Int32>30</System:Int32>
                    </ComboBox>
                    <inputToolkit:NumericUpDown Grid.Column="1" Grid.Row="13" Height="20" Margin="0,2,8,2" HorizontalAlignment="Right" Maximum="5" Value="{Binding Mode=TwoWay, Path=ComparisonAdvancedConstraintsLevel}" />
                    <inputToolkit:NumericUpDown Grid.Column="2" Grid.Row="13" Height="20" HorizontalAlignment="Left" Margin="0,2,8,2" Value="{Binding Mode=TwoWay, Path=DisplayAdvancedConstraintsLevel}" Maximum="5" />
                </Grid>
            </controls:TabItem>
            <controls:TabItem Header="Tools">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" MinWidth="250"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Row="0" Text="Cycle Analysis:" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,0" VerticalAlignment="Top" Width="Auto" TextWrapping="Wrap"/>
                    <Button Grid.Row="1" Content="Cycle Analyzer" Margin="2,2,2,2" Click="CycleAnalyzer_Click" />
                    <Button Grid.Row="2" Content="Hot Streak Utilization" Margin="2,2,2,2" Click="HotStreakUtilization_Click" />
                    <TextBlock Grid.Row="4" Text="Editors:" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,0" VerticalAlignment="Top" Width="Auto" TextWrapping="Wrap"/>
                    <Button Grid.Row="5" Content="Cooldown Editor" Margin="2,2,2,2" Click="CooldownEditor_Click" IsEnabled="False" />
                    <TextBlock Grid.Row="8" Text="Performance:" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,0" VerticalAlignment="Top" Width="Auto" TextWrapping="Wrap"/>
                    <Button Grid.Row="10" Content="Calculation Timing" Margin="2,2,2,2" Click="CalculationTiming_Click" />
                </Grid>
            </controls:TabItem>
            <controls:TabItem Header="Solver Log">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <TextBox VerticalAlignment="Stretch" IsReadOnly="True" Margin="2,2,2,2" TextWrapping="Wrap" x:Name="AdvancedSolverLog" Grid.Row="0" VerticalScrollBarVisibility="Auto"/>
                    <Button x:Name="AdvancedSolverCancel" Content="Cancel Calculations" Click="AdvancedSolverCancel_Click" Grid.Row="1" HorizontalAlignment="Right" Margin="2,2,2,2" />
                </Grid>
            </controls:TabItem>
        </controls:TabControl>
    </Grid>
</UserControl>
