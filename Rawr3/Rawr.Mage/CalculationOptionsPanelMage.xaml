<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Rawr.Mage" 
    xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    mc:Ignorable="d" xmlns:System="clr-namespace:System;assembly=mscorlib"
    xmlns:inputToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Input.Toolkit"
    x:Class="Rawr.Mage.CalculationOptionsPanelMage" 
    Width="Auto" Height="Auto" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <local:CalculationOptionsMage x:Key="CalculationOptionsMageDataSource" d:IsDataSource="True"/>
    </UserControl.Resources>
    <Grid x:Name="LayoutRoot" Background="White" DataContext="{Binding Source={StaticResource CalculationOptionsMageDataSource}}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <controls:TabControl x:Name="OptionsTab" SelectionChanged="OptionsTab_SelectionChanged">
            <controls:TabItem Header="Character">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.6*"/>
                        <ColumnDefinition Width="0.4*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Player Level: " TextWrapping="Wrap" ToolTipService.ToolTip=""/>
                    <ComboBox Margin="0,2,0,2" Grid.Column="1" Grid.Row="0" Height="Auto" SelectedItem="{Binding Mode=TwoWay, Path=PlayerLevel, UpdateSourceTrigger=Explicit}">
                        <System:Int32>70</System:Int32>
                        <System:Int32>71</System:Int32>
                        <System:Int32>72</System:Int32>
                        <System:Int32>73</System:Int32>
                        <System:Int32>74</System:Int32>
                        <System:Int32>75</System:Int32>
                        <System:Int32>76</System:Int32>
                        <System:Int32>77</System:Int32>
                        <System:Int32>78</System:Int32>
                        <System:Int32>79</System:Int32>
                        <System:Int32>80</System:Int32>
                    </ComboBox>
                    <TextBlock Height="Auto" Grid.Row="2" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Cast Latency: *" TextWrapping="Wrap" ToolTipService.ToolTip="Effective latency in seconds. The value should include the benefit of built-in /stopcasting."/>
                    <TextBox Height="20" Margin="0,2,0,2" Grid.Row="2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=LatencyCast}" TextWrapping="Wrap"/>
                    <TextBlock Height="Auto" Grid.Row="3" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="GCD Latency: *" TextWrapping="Wrap" ToolTipService.ToolTip="Effective latency in seconds. The value should include the benefit of built-in /stopcasting."/>
                    <TextBox Height="20" Margin="0,2,0,2" Grid.Row="3" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=LatencyGCD}" TextWrapping="Wrap"/>
                    <TextBlock Height="Auto" Grid.Row="4" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Channel Latency: *" TextWrapping="Wrap" ToolTipService.ToolTip="Effective latency in seconds. The value should include the benefit of built-in /stopcasting."/>
                    <TextBox Height="20" Margin="0,2,0,2" Grid.Row="4" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=LatencyChannel}" TextWrapping="Wrap"/>
                </Grid>
            </controls:TabItem>
            <controls:TabItem Header="Spells &amp; Buffs">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.6*"/>
                        <ColumnDefinition Width="0.4*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Automatic Armor: *" TextWrapping="Wrap" ToolTipService.ToolTip="Optimize between mage and molten armor. Disable to improve performance." Grid.Row="0"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Text="Heroism/Bloodlust: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable use of Heroism." Grid.Row="1" d:LayoutOverrides="GridBox"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Power Infusion: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable use of Power Infusion (one priest only)." Grid.Row="2"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Potion of Wild Magic: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable use of Potion of Wild Magic." Grid.Row="3"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Potion of Speed: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable use of Potion of Speed." Grid.Row="4"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Flame Cap: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable use of Flame Caps." Grid.Row="5"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Mirror Image: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable use of Mirror Image." Grid.Row="6"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Maintain Snare: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable to only use cycles that refresh snare debuff." Grid.Row="7"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Maintain Scorch: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable to only use cycles that refresh scorch debuff." Grid.Row="8"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Heroism Control:" TextWrapping="Wrap" ToolTipService.ToolTip="" Grid.Row="9"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Innervate: *" TextWrapping="Wrap" ToolTipService.ToolTip="Number of innervates received." Grid.Row="10"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Mana Tide Totem: *" TextWrapping="Wrap" ToolTipService.ToolTip="Number of Mana Tide Totems cast in your group (a resto shaman can cast one every 5 minutes)." Grid.Row="11"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Average Cooldowns: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable to make cooldown maximum usage gradually increases with fight duration (i.e. max 1.66 Arcane Power activations on a 5 minute fight)." Grid.Row="12"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Disable Cooldowns: *" TextWrapping="Wrap" ToolTipService.ToolTip="Disables other activatable cooldowns (talents and trinkets)." Grid.Row="13"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Evocation: *" TextWrapping="Wrap" ToolTipService.ToolTip="You can uncheck this to disable use of Evocation." Grid.Row="14"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="2T10 Evocation: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable this to use Evocation with 2T10 buff, only has effect if you actually have 2T10 equipped." Grid.Row="15"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Mana Potion: *" TextWrapping="Wrap" ToolTipService.ToolTip="You can uncheck this to disable use of Mana Potions." Grid.Row="16"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Mana Gem: *" TextWrapping="Wrap" ToolTipService.ToolTip="You can uncheck this to disable use of Mana Gems." Grid.Row="17"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Custom Spell Mix: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable and configure a special custom mix of spells." Grid.Row="18"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Custom Mix Only: *" TextWrapping="Wrap" ToolTipService.ToolTip="Disable all spell cycles except custom mix." Grid.Row="19"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Snared Time (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Percentage of time that the target is snared by someone else, cycles that provide snare override this to 1." Grid.Row="20"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Frostbite Utilization (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Utilization of 1 corresponds to no diminishing returns, only applies to Blizzard." Grid.Row="21"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Focus Magic Rate: *" TextWrapping="Wrap" ToolTipService.ToolTip="Normalized Crit rate of Focus Magic target, compute as [1 - (1 - crit chance) ^ (1 / cast time)] (example 50% crit change, 3 sec cast time =&gt; 0.206)" Grid.Row="22"/>
                    <TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Warlock Spell Power: *" TextWrapping="Wrap" ToolTipService.ToolTip="Spell Power used for Demonic Pact buff value." Grid.Row="23"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="0" IsChecked="{Binding Mode=TwoWay, Path=AutomaticArmor}"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="1" IsChecked="{Binding Mode=TwoWay, Path=HeroismAvailable}" d:LayoutOverrides="Height, VerticalMargin"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="2" IsChecked="{Binding Mode=TwoWay, Path=PowerInfusionAvailable}" d:LayoutOverrides="Height, VerticalMargin"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="3" IsChecked="{Binding Mode=TwoWay, Path=PotionOfWildMagic}" d:LayoutOverrides="Height, VerticalMargin"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="4" IsChecked="{Binding Mode=TwoWay, Path=PotionOfSpeed}" d:LayoutOverrides="Height, VerticalMargin"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="5" IsChecked="{Binding Mode=TwoWay, Path=FlameCap}" d:LayoutOverrides="Height, VerticalMargin"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="6" IsChecked="{Binding Mode=TwoWay, Path=MirrorImageEnabled}" d:LayoutOverrides="Height, VerticalMargin"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="7" IsChecked="{Binding Mode=TwoWay, Path=MaintainSnare}" d:LayoutOverrides="Height, VerticalMargin"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="8" IsChecked="{Binding Mode=TwoWay, Path=MaintainScorch}" d:LayoutOverrides="Height, VerticalMargin"/>
                    <ComboBox Margin="0,2,0,2" Grid.Column="1" Grid.Row="9" SelectedIndex="{Binding Mode=TwoWay, Path=HeroismControl}" Height="Auto">
                        <ListBoxItem Content="Optimal"/>
                        <ListBoxItem Content="Before 35%"/>
                        <ListBoxItem Content="No Cooldowns"/>
                        <ListBoxItem Content="After 35%"/>
                    </ComboBox>
                    <TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=Innervate}" TextWrapping="Wrap" Grid.Row="10"/>
                    <TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=ManaTide}" TextWrapping="Wrap" Grid.Row="11"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="12" IsChecked="{Binding Mode=TwoWay, Path=AverageCooldowns}"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="13" IsChecked="{Binding Mode=TwoWay, Path=DisableCooldowns}"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="14" IsChecked="{Binding Mode=TwoWay, Path=EvocationEnabled}"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="15" IsChecked="{Binding Mode=TwoWay, Path=Enable2T10Evocation}"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="16" IsChecked="{Binding Mode=TwoWay, Path=ManaPotionEnabled}"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="17" IsChecked="{Binding Mode=TwoWay, Path=ManaGemEnabled}"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="19" IsChecked="{Binding Mode=TwoWay, Path=CustomSpellMixOnly}"/>
                    <CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="18" IsChecked="{Binding Mode=TwoWay, Path=CustomSpellMixEnabled}" Width="20" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <Button Margin="0,2,0,2" Content="..." Grid.Column="1" Grid.Row="18" Width="96" HorizontalAlignment="Right" VerticalAlignment="Stretch" Click="CustomSpellMix_Click" />
                    <TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=SnaredTime}" TextWrapping="Wrap" Grid.Row="20"/>
                    <TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=FrostbiteUtilization}" TextWrapping="Wrap" Grid.Row="21"/>
                    <TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=FocusMagicTargetCritRate}" TextWrapping="Wrap" Grid.Row="22"/>
                    <TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=WarlockSpellPower}" TextWrapping="Wrap" Grid.Row="23"/>
                </Grid>
            </controls:TabItem>
            <controls:TabItem Header="Fight">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.6*" />
                        <ColumnDefinition Width="0.4*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Grid.Row="0" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Target Level: " TextWrapping="Wrap" ToolTipService.ToolTip="" />
                    <TextBlock Grid.Column="0" Grid.Row="1" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Fight Duration (sec):" TextWrapping="Wrap" ToolTipService.ToolTip="" />
                    <TextBlock Grid.Column="0" Grid.Row="2" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Tps Limit: *" TextWrapping="Wrap" ToolTipService.ToolTip="Threat per second limit. Set to 0 to disable threat constraint." />
                    <TextBlock Grid.Column="0" Grid.Row="3" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Dps time (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Time spent casting (default value is 1 = 100%). This value is analogous to Dps time in WWS reports, use it to indicate how much time is lost on movement and waiting." />
                    <TextBlock Grid.Column="0" Grid.Row="4" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Fragmentation (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Indicates how fragmented the dps time is and translates to how much out of five second rule time you get when not dpsing (higher value means more time inside five second rule)." />
                    <TextBlock Grid.Column="0" Grid.Row="5" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Interrupt Frequency: *" TextWrapping="Wrap" ToolTipService.ToolTip="Frequency of pushbacks expressed as number of interrupts per second. An example value for Teron Gorefiend would be between 0.05 and 0.1." />
                    <TextBlock Grid.Column="0" Grid.Row="6" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Molten Fury (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Percentage of time spent in molen fury range (i.e. &lt; 35% boss health). Default value of 0.3 indicates that 30% of time is spent in molten fury range due to raid dps stacking in execute range." />
                    <TextBlock Grid.Column="0" Grid.Row="7" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Movement Frequency: *" TextWrapping="Wrap" ToolTipService.ToolTip="Frequency of movement events (expressed as average time between movement events, only includes movement that prevents you from attacking such as being out of range)." />
                    <TextBlock Grid.Column="0" Grid.Row="8" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Movement Duration: *" TextWrapping="Wrap" ToolTipService.ToolTip="Duration of movement events (expressed as time in seconds you would have to move without any movement increasing effects, only includes movement that prevents you from attacking such as being out of range)." />
                    <TextBlock Grid.Column="0" Grid.Row="9" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Aoe Target Level: *" TextWrapping="Wrap" ToolTipService.ToolTip="Level of aoe targets." />
                    <TextBlock Grid.Column="0" Grid.Row="10" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Aoe Targets: *" TextWrapping="Wrap" ToolTipService.ToolTip="Number of simultaneous aoe targets." />
                    <TextBlock Grid.Column="0" Grid.Row="11" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Aoe Duration (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Time spent aoeing. Value of 0.1 for example would mean 10% of time is spent aoeing." />
                    <TextBlock Grid.Column="0" Grid.Row="12" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Arcane Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." />
                    <TextBlock Grid.Column="0" Grid.Row="13" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Fire Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." />
                    <TextBlock Grid.Column="0" Grid.Row="14" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Frost Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." />
                    <TextBlock Grid.Column="0" Grid.Row="15" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Nature Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." />
                    <TextBlock Grid.Column="0" Grid.Row="16" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Shadow Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." />
                    <TextBlock Grid.Column="0" Grid.Row="17" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Holy Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." />
                    <TextBlock Grid.Column="0" Grid.Row="18" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Drinking Time: *" TextWrapping="Wrap" ToolTipService.ToolTip="Setting this to a low value simulates having a limited time to regen mana on chain pulls (value is in seconds, optimizer has the option to continue drinking into the pull if it increases overall damage done, but that time counts toward total time)." />
                    <TextBlock Grid.Column="0" Grid.Row="19" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Target Damage: *" TextWrapping="Wrap" ToolTipService.ToolTip="Instead of maximizing damage done this minimizes time it takes to deal the specified damage if value is greater than 0 (for best results enable Average Cooldowns)." />
                    <TextBlock Grid.Column="0" Grid.Row="20" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Farming Mode: *" TextWrapping="Wrap" ToolTipService.ToolTip="Includes time it takes to regen mana/health after encounter, use in combination with target damage." />
                    <TextBlock Grid.Column="0" Grid.Row="21" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Unlimited Mana: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable to completely ignore mana in evaluations." />
                    <TextBlock Grid.Column="0" Grid.Row="22" Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="PVP: *" TextWrapping="Wrap" ToolTipService.ToolTip="When enabled follows PVP rules." />
                    <ComboBox Grid.Column="1" Grid.Row="0" Margin="0,2,0,2" Height="Auto" SelectedItem="{Binding Mode=TwoWay, Path=TargetLevel, UpdateSourceTrigger=Explicit}">
                        <System:Int32>70</System:Int32>
                        <System:Int32>71</System:Int32>
                        <System:Int32>72</System:Int32>
                        <System:Int32>73</System:Int32>
                        <System:Int32>74</System:Int32>
                        <System:Int32>75</System:Int32>
                        <System:Int32>76</System:Int32>
                        <System:Int32>77</System:Int32>
                        <System:Int32>78</System:Int32>
                        <System:Int32>79</System:Int32>
                        <System:Int32>80</System:Int32>
                        <System:Int32>81</System:Int32>
                        <System:Int32>82</System:Int32>
                        <System:Int32>83</System:Int32>
                    </ComboBox>
                    <ComboBox Grid.Column="1" Grid.Row="9" Margin="0,2,0,2" Height="Auto" SelectedItem="{Binding Mode=TwoWay, Path=AoeTargetLevel, UpdateSourceTrigger=Explicit}">
                        <System:Int32>70</System:Int32>
                        <System:Int32>71</System:Int32>
                        <System:Int32>72</System:Int32>
                        <System:Int32>73</System:Int32>
                        <System:Int32>74</System:Int32>
                        <System:Int32>75</System:Int32>
                        <System:Int32>76</System:Int32>
                        <System:Int32>77</System:Int32>
                        <System:Int32>78</System:Int32>
                        <System:Int32>79</System:Int32>
                        <System:Int32>80</System:Int32>
                        <System:Int32>81</System:Int32>
                        <System:Int32>82</System:Int32>
                        <System:Int32>83</System:Int32>
                    </ComboBox>
                    <TextBox Grid.Column="1" Grid.Row="1" Height="20" Margin="0,2,0,2" Text="{Binding Mode=TwoWay, Path=FightDuration, UpdateSourceTrigger=Default}" TextWrapping="Wrap" />
